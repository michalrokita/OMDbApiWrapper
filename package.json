{
  "name": "omdb-api-wrapper",
  "version": "1.0.0",
  "description": "A JS wrapper around www.omdbapi.com API.",
  "scripts": {
    "build": "NODE_ENV=development webpack --config config/webpack.config.js",
    "build:prod": "NODE_ENV=production webpack --config config/webpack.config.js",
    "test:unit": "jest --config config/jest.config.js",
    "prepare": "npm run build",
    "publish": "npm publish || true"
  },
  "main": "build/index.js",
  "author": "Michal Rokita <michal.rokita17@gmail.com>",
  "license": "MIT",
  "private": false,
  "keywords": [
    "library",
    "omdb",
    "api"
  ],
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-transform-modules-umd": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.1.0",
    "eslint": "^7.13.0",
    "eslint-plugin-jest": "^24.1.0",
    "jest": "^26.6.3",
    "webpack": "^5.4.0",
    "webpack-cli": "^4.2.0"
  }
}
